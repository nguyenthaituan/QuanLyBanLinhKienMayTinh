@model QuanLyBanLinhKienMayTinh.Models.NHASANXUAT

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Layout/_AdminLayout.cshtml";
}

<h2>Xóa</h2>
<div>
    <h4>NHASANXUAT</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên nhà sản xuất
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenNSX)
        </dd>

        <dt>
            Thông tin nhà sản xuất:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ThongTin)
        </dd>

        <dt>
            Logo:
        </dt>

        <dd>
            <td rowspan="8"><img src="~/Content/HinhAnhSP/@Html.DisplayFor(model => model.Logo)" width="100" height="100" /></td>
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-default" /> |
            @Html.ActionLink("Quay lại", "Index")
        </div>
    }
</div>
